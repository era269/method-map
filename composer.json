{
    "name": "era269/method-map",
    "type": "library",
    "description": "Object method mapping by parameter type",
    "keywords": ["OOP", "Object"],
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^7.1",
        "cache/array-adapter": "^1.1",
        "psr/simple-cache": "^1.0",
      "": ""
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.85",
        "phpstan/phpstan-phpunit": "^0.12.18",
        "phpunit/phpunit": "^7.0"
    },
    "authors": [
        {
            "name": "Yuriy Antonuyk",
            "email": "era269@ukr.net"
        }
    ],
    "config": {
        "platform": {
            "php": "7.1"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Era269\\MethodMap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Era269\\MethodMap\\Tests\\": "tests"
        }
    }
}
